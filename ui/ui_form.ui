}

/* 设置表头样式 */
QHeaderView::section {
    background-color: #4796f0;  /* 表头背景颜色 */
    color: white;               /* 表头文字颜色 */
    padding: 5px;               /* 内边距 */
    border: 1px solid #c0c0c0;  /* 表头边框 */
}

/* 设置单元格的样式 */
QTableWidget::item {
    border: 1px solid #c0c0c0;  /* 单元格边框颜色 */
    padding: 5px;               /* 单元格内边距 */
}

/* 设置选中单元格的样式 */
QTableWidget::item:selected {
    background-color: #4796f0;  /* 选中的单元格背景颜色 */
    color: white;               /* 选中的单元格文字颜色 */
}

/* 设置TabWidget的样式 */
QTabWidget::pane {
    border: 2px solid #4796f0;  /* Tab外框颜色 */
    background: #e0e0e0;        /* 背景颜色 */
    padding: 10px;              /* 内边距，增加Tab之间的间距 */
    border-radius: 10px;        /* 圆角 */
}

QTabBar::tab {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, 
                                stop: 0 #f0f0f0, stop: 1 #d0d0d0); /* 渐变背景 */
    border: 1px solid #a0a0a0;  /* 边框颜色 */
    padding: 10px;              /* 内边距，控制tab的大小 */
    margin-right: 5px;          /* Tab之间的间距 */
    border-radius: 5px;         /* 圆角 */
    color: #333;                /* 文字颜色 */
}

/* 设置鼠标悬停时的Tab样式 */
QTabBar::tab:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, 
                                stop: 0 #d0d0d0, stop: 1 #a0a0a0); /* 悬停时的渐变背景 */
}

/* 设置选中的Tab样式 */
QTabBar::tab:selected {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, 
                                stop: 0 #4796f0, stop: 1 #2077c0); /* 选中时的渐变背景 */
    color: white;               /* 选中时的文字颜色 */
}

/* 设置Tab选项卡的最低宽高 */
QTabBar::tab {
    min-width: 100px;           /* Tab最小宽度 */
    min-height: 30px;           /* Tab最小高度 */
}
